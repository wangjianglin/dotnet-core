<?xml version="1.0"?>
<!--此处配置不可变信息-->
<configuration>
  <!--读取服务器连接地址-->
  <configSections>
    <sectionGroup name="Domain">
      <section name="TestDomain" type="System.Configuration.NameValueSectionHandler" />
      <section name="OfficialDomain" type="System.Configuration.NameValueSectionHandler" />
      <section name="KeepJoin" type="System.Configuration.NameValueSectionHandler"/>
    </sectionGroup>

    <sectionGroup name="Cache">
      <section name="CacheSize" type="System.Configuration.NameValueSectionHandler" />
      <section name="CacheRound" type="System.Configuration.NameValueSectionHandler" />
    </sectionGroup>
  </configSections>

  <Domain>
    <TestDomain>
      <add key="16测试服务器" value="http://192.168.1.16:8080/web" />
      <add key="18测试服务器" value="http://192.168.1.18:8080/web" /> 
    </TestDomain>
    <OfficialDomain>
      <add key="电信接入" value="http://dx.epcbm.com/web" />
      <add key="联通接入" value="http://lt.epcbm.com/web" /> 
    </OfficialDomain>
    <KeepJoin>
      <add key="一分钟一次" value="1"/>
      <add key="一分钟两次" value="2"/>
      <add key="一分钟三次" value="3"/>
      <add key="一分钟四次" value="4"/>
      <add key="一分钟五次" value="5"/>
      <add key="永不发送" value="0"/>
    </KeepJoin>
  </Domain>
  <Cache>
  <CacheSize>
    <add key="100M" value="102400" />
    <add key="200M" value="204800" />
    <add key="300M" value="307200" />
    <add key="400M" value="409600" />
    <add key="500M" value="512000" />
    <add key="不限大小" value="0" />
  </CacheSize>
  <CacheRound>
    <add key="一周" value="7" />
    <add key="三周" value="21" />
    <add key="一个月" value="30" />
    <add key="三个月" value="90" />
    <add key="永不清理" value="0" />
  </CacheRound>
  </Cache>
  
  <appSettings>
    <add key="Net#Server" value="http://dx.epcbm.com/web"/>
    <!--当前项目，生成的dll所在目录-->
    <add key="System#ProcedurePath" value="lib"/>
    <!--填写运行项目的进程名称，如ECM.ini,Adows.ini，后缀不一定得是ini-->
    <add key="configName" value="ECM.ini"/>
    <add key="AppName" value="ECM"/>
  </appSettings>
   
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Telerik.Windows.Controls.Navigation" publicKeyToken="5803CFA389C90CE7" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-2012.2.607.40" newVersion="2012.2.607.40"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Telerik.Windows.Controls" publicKeyToken="5803CFA389C90CE7" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-2012.2.607.40" newVersion="2012.2.607.40"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Telerik.Windows.Data" publicKeyToken="5803CFA389C90CE7" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-2012.2.607.40" newVersion="2012.2.607.40"/>
      </dependentAssembly>
      <probing privatePath="lib"/>
    </assemblyBinding>
  </runtime>
</configuration>
